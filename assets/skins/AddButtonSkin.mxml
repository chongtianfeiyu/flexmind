<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" 
		
		xmlns:fx="http://ns.adobe.com/mxml/2009" 
		
		xmlns:d="http://ns.adobe.com/fxg/2008/dt">
	<fx:Metadata>[HostComponent("spark.components.Button")]
		
	</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect alpha="1" height="28" id="rect2" radiusX="3" width="120" 
			
			y="0.5" x="0.5">
		<s:stroke>
			<s:SolidColorStroke caps="none" color="#999999" 
								
								joints="miter" miterLimit="4" weight="1" color.over="#FFFFFF"/>
		</s:stroke>
		<s:filters>
			<s:GlowFilter alpha="1.0" blurX="3" blurY="3" 
						  
						  color="0x5380d0" inner="false" knockout="false" quality="2" strength="2" 
						  
						  includeIn="up,down"/>
		</s:filters>
		<s:fill.down>
			<s:SolidColor color="#FFFFFF"/>
		</s:fill.down>
		<s:fill.disabled>
			<s:SolidColor color="#FFFFFF"/>
		</s:fill.disabled>
		<s:fill.up>
			<s:SolidColor color="#FFFFFF"/>
		</s:fill.up>
		<s:fill.over>
			<s:SolidColor color="#ADD5F7"/>
		</s:fill.over>
	</s:Rect>
	<s:BitmapImage height="28" smooth="true" source="@Embed('assets/images/save.png')" width="28" y="1" 
											   
											   blendMode.disabled="luminosity" x="45"/>
	<s:RichText color="#00A700" fontFamily="Arial" fontSize="28" 
				
				fontWeight="bold" id="richtext1" tabStops="S0" text="+"   y="9" blendMode.down="hardlight" 
				
				blendMode.disabled="luminosity" x="64">
		<s:filters>
			<s:DropShadowFilter alpha="1" angle="45.0" 
								
								blurX="8" blurY="8" color="#FFFFFF" distance="1" hideObject="false" 
								
								inner="false" knockout="false" quality="2" strength="2"/>
			<s:BevelFilter angle="45.0" blurX="4.0" 
						   
						   blurY="4.0" distance="2" highlightAlpha="1.0" highlightColor="0xFFFFFF" 
						   
						   knockout="false" quality="2" shadowAlpha="1.0" shadowColor="#FFFFFF" 
						   
						   strength="1" type="inner"/>
		</s:filters>
	</s:RichText>
</s:Skin>
