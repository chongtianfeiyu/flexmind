<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage height="14" smooth="true" source="@Embed('assets/images/cancel.png')" width="14" y="1" x="1" blendMode.disabled="color" blendMode.up="luminosity">
		<s:filters>
			<s:GlowFilter includeIn="over" blurX="6.0" blurY="6.0" inner="false" color="#FFFFFF" strength="1" alpha="1.0" quality="2" knockout="false"/>
			<s:GlowFilter includeIn="up" blurX="6.0" blurY="6.0" inner="false" color="#FFFFFF" strength="2" alpha="1.0" quality="2" knockout="false"/>
		</s:filters>
	</s:BitmapImage>
	<s:Rect height="15" includeIn="down,over" width="15" x="0.5" y="0.5" visible.over="false">
		<s:stroke>
			<s:SolidColorStroke caps="none" color="#999999" joints="miter" miterLimit="4" weight="1"/>
		</s:stroke>
	</s:Rect>
</s:Skin>
